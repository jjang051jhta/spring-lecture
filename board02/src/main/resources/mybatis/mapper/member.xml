<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jjang051.board02.dao.MemberDao">
    <insert id="signup" parameterType="MemberDto">
        insert into member (id,userID,userPW,userName,userEmail) values
        (member_seq.nextval,#{userID},#{userPW},#{userName},#{userEmail})
    </insert>
    <select id="idCheck" parameterType="MemberDto" resultType="int">
        SELECT count(*) AS count FROM MEMBER WHERE userid = #{userID}
    </select>
</mapper>